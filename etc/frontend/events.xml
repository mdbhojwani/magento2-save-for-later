<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="customer_login">
        <observer name="saveforlater" instance="Mdbhojwani\SaveForLater\Observer\CustomerLogin" />
    </event>
    <event name="customer_logout">
        <observer name="saveforlater" instance="Mdbhojwani\SaveForLater\Observer\CustomerLogout" />
    </event>
</config>
